{
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "export-mac": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll",
    "export": "next build && next export && type nul > out/.nojekyll",
    "deploy": "gh-pages -d out -t true",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate-css": "postcss styles/globals.css -o public/globals.css --watch",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@storybook/jest": "^0.0.10",
    "@storybook/node-logger": "^6.5.9",
    "clsx": "^1.1.1",
    "lodash": "^4.17.21",
    "next": "^12.1.7-canary.19",
    "postcss-cli": "^10.0.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-spring": "^9.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^3.0.0-alpha.1",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^12.12.21",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "gh-pages": "^4.0.0",
    "husky": "1.1.2",
    "lint-staged": "7.2.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.0.24",
    "typescript": "4.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
